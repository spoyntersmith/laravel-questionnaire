{
    "name": "pentangle/laravel-questionnaire",
    "description": "A robust form system which records responses to questionnaires",
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "authors": [
        {
            "name": "sean.poynter-smith",
            "email": "sean.poynter-smith@pentangletechnology.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Pentangle\\Questionnaire\\": "src"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Pentangle\\Questionnaire\\Test\\": "tests"
        }
    },
    "require": {}
}
